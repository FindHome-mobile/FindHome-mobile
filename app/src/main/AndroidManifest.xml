<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.FinHome">
        <activity
            android:name="com.example.devmobile.WelcomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.devmobile.LoginActivity"
            android:exported="false" />

        <activity
            android:name="com.example.devmobile.RegisterActivity"
            android:exported="false" />

        <activity
            android:name="com.example.devmobile.DashboardActivity"
            android:exported="false" />

        <activity
            android:name="com.example.devmobile.ProfileActivity"
            android:exported="false" />

        <activity
            android:name="com.example.devmobile.ListingsActivity"
            android:exported="false" />

        <activity
            android:name="com.example.devmobile.AdminActivity"
            android:exported="false" />

        <activity
            android:name="com.example.devmobile.AnnonceDetailActivity"
            android:exported="false" />

        <activity
            android:name="com.example.devmobile.CreateAnnonceActivity"
            android:exported="false" />

        <activity
            android:name="com.example.devmobile.FavoritesActivity"
            android:exported="false" />

        <activity
            android:name="com.example.devmobile.MyAnnoncesActivity"
            android:exported="false" />

    </application>

</manifest>